    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "env": {
                "MEDIA_ROOT": "media"
            },
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/backend/manage.py"
        },
        {
            "name": "Python Debugger: Tags",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "load",
                "tags"
            ],
            "django": true,
            "env": {
                "MEDIA_ROOT": "media"
            },
            "cwd": "${workspaceFolder}/backend",
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/backend/manage.py"

        }
    ]
}